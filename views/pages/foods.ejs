<!DOCTYPE html>
<!--[if IE 9]><html class="ie ie9"> <![endif]-->
<html>
<head>
     <% include ../partials/head %>
</head>

<body>

<!--[if lte IE 8]>
    <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a>.</p>
<![endif]-->

    <!-- Header ================================================== -->
    <header>
    <div class="container-fluid">
        <% include ../partials/header %>
    </div><!-- End container -->
    </header>
    <!-- End Header ===============<link href='https://fonts.googleapis.com/css/?family=Lato:400,700,900,400italic,700italic,300,300italic' rel='stylesheet' type='text/css'>================================ -->

<!-- SubHeader =============================================== -->
<section class="parallax-window" id="short" data-parallax="scroll" data-image-src="../static/img/burger.jpg" data-natural-width="1400" data-natural-height="350">
    <div id="subheader">
	<div id="sub_content">
    	<h1 style="color:black"><b><%=foods.length%> sonuç bulundu</b></h1></strong>
    </div><!-- End sub_content -->
</div><!-- End subheader -->
</section><!-- End section -->
<!-- End SubHeader ============================================ -->

    <div id="position">
        <div class="container">
            <ul>
                <li><a href="/">Anasayfa</a></li>
                <li><a href="/">Yemekler</a></li>
                <li>Sonuçlar</li>
            </ul>
        </div>
    </div><!-- Position -->


<!-- Content ================================================== -->
<div class="container margin_60_35">
	<div class="row">

    <% for(var i=0; i<foods.length; i++) {%>
      <div class="strip_list wow fadeIn" data-wow-delay="0.1s">
        <div class="row">
          <div class="col-md-9 col-sm-9">
            <div class="desc">
              <div class="thumb_strip">
                <a href="/find/<%= foods[i]._id %>"><img src="static/img/yemek.jpg" alt=""></a>
              </div>
              <h3><%= foods[i].name%></h3>
              <div class="type">
                <%= foods[i].category%>
              </div>
              <div class="type">
                 <%for(var j=0; j<(foods[i].ingredients).length; j++) {%>
                   -<%- (foods[i].ingredients[j]).name %> &nbsp &nbsp
                            <% } %>
              </div>
              <ul>
                <li>Malzemeler Tamam mı?<i class="icon_check_alt2 ok"></i></li>
                <li>Uzun süren bir yemek mi?<i class="icon_check_alt2 no"></i></li>
              </ul>
            </div>
          </div>
          <div class="col-md-3 col-sm-3">
            <div class="go_to">
              <div>
                <a href="/find/<%= foods[i]._id %>" class="btn_1">Detay</a>
              </div>
            </div>
          </div>
        </div><!-- End row-->
      </div><!-- End strip_list-->
       <% } %>


      <a href="#0" class="load_more_bt wow fadeIn" data-wow-delay="0.2s">Daha Fazla Yükle...</a>
	<!-- End col-md-9-->

	</div><!-- End row -->
</div><!-- End container -->
<!-- End Content =============================================== -->

<!-- Footer ================================================== -->
	<footer>
        <% include ../partials/footer %>
    </footer>
<!-- End Footer =============================================== -->

<div class="layer"></div><!-- Mobile menu overlay mask -->
<script src="static/js/jquery-1.11.2.min.js"></script>
<script src="static/js/common_scripts_min.js"></script>
<script src="static/js/functions.js"></script>


</body>
</html>
